<!-- Changes after -->
<wrapper>
<h2 class="center">Users Table</h2>
<div class="center">
  <a class="button" href="/users/new">Create User</a>

</div>
<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Account Type</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% user.forEach(user => { %>
    <tr>
      <td>
        <a href="<%= `/users/${user._id}` %>">
          <%= user.email %> 
        </a>
      </td>
      <td>
          <% if(user.accountType == "Business"){ %> 

            <a href="<%='/users/${user._id}/createBusinessProfile'%> ">
            <%= user.accountType %>
           
        </a>
        <% } else { %> 
            <a href="/users">
                <% user.accountType %>
            </a>
            <% } %>
         

      </td>
      <td>
        <a href="<%=`users/${user._id}/edit`%>">
          Edit
        </a>
      </td>
      <td>
        <a href="<%= `users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')" a>Delete</a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
</wrapper>
